regex=grsec: denied ptrace of \(([^:]+):(\d+)\) by \(([^:]+):(\d+)\) UID\((\d+)\) EUID\((\d+)\), parent \(([^:]+):(\d+)\) UID\((\d+)\) EUID\((\d+)\);\
	classification(0).name=Attempted ptrace;assessment.impact.completion=failed;assessment.impact.type=other;assessment.impact.severity=high;\
	source(0).process.name=$3;source(0).process.pid=$4;target(0).process.name=$1;target(0).process.pid=$2;\
	source(0).user.category=application;source(0).user.userid(1).type=current-user;source(0).user.userid(1).number=$3;\
	source(0).user.userid(1).type=original-user;source(0).user.userid(1).number=$4;\
