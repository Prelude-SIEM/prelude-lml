#Oct 27 23:22:53 shadowcat nagios: SERVICE ALERT: adtcadb;SQL Server;CRITICAL;HARD;3;Socket timeout after 10 seconds
regex=nagios: SERVICE ALERT: (\w+).([\w\s]+).(CRITICAL|WARNING|OK).HARD.\d.(.+)$; \
 class.name=Service $3; \
 id=1100; \
 revision=1; \
 class.origin=vendor-specific; \
 impact.severity=medium; \
 impact.type=other; \
 impact.description=Service $2 on $1 reported status $3 with $4; \
 last

#Oct 28 11:42:33 shadowcat nagios: HOST ALERT: rcts2;UP;HARD;2;PING OK - Packet loss = 0%, RTA = 0.26 ms
regex=nagios: HOST ALERT: (\w+).(DOWN|UP).HARD.\d.(.+)$; \
 class.name=Host $2; \
 id=1101; \
 revision=1; \
 class.origin=vendor-specific; \
 impact.severity=medium; \
 impact.type=other; \
 impact.description=Host $1 reported status $2 with $3; \
 last
