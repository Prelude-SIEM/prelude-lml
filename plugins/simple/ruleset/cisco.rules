# Cisco router support for Prelude-LML
# No log sample; please submit
regex=.*SEC-[0-9]+-IPACCESSLOGP: list ([0-9]+) denied ([a-z]+) ([0-9\.]+)\(([0-9]+)\).*-> ([0-9\.]+)\(([0-9]+)\).*; \
 classification(0).name=Cisco ip access; \
 id=500; \
 revision=1; \
 classification(0).origin=vendor-specific; \
 assessment.impact.completion = failed; \
 assessment.impact.type = other; \
 assessment.impact.severity = medium; \
 assessment.impact.description= Someone tried to bypass access-list #$1; \
  \
 source(0).node.address(0).category=ipv4-addr; \
 source(0).node.address(0).address = $3; \
 source(0).service.port = $4; \
 source(0).service.protocol = $2; \
  \
 target(0).node.address(0).category=ipv4-addr; \
 target(0).node.address(0).address = $5; \
 target(0).service.port = $6; \
 target(0).service.protocol = $2; \
 last

# No log sample; please submit
regex=.*RCMD-[0-9]+-RSHPORTATTEMPT:.* from ([0-9\.]+).*; \
 classification(0).name=RSH attempt; \
 id=501; \
 revision=1; \
 classification(0).origin=vendor-specific; \
 assessment.impact.completion = failed; \
 assessment.impact.type = other; \
 assessment.impact.severity = medium; \
 assessment.impact.description= Someone tried to connect to rshell; \
  \
 source(0).node.address(0).category=ipv4-addr; \ 
 source(0).node.address(0).address = $1; \
 last
