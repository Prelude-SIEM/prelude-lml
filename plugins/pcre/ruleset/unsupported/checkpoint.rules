# II. SmartDefense
###

# 1. Port scanning
# No sample log entry; please submit
regex=product: SmartDefense. dst: ([\d\.]+). attack: Port Scanning. src: ([\d\.]+); \
 classification(0).name=Port scan detection; \
 id=112; \
 revision=1; \
 classification(0).origin=vendor-specific; \
 assessment.impact.severity=low; \
 assessment.impact.type=recon; \
 assessment.impact.description=Checkpoint SmartDefense has detected a port scan from $2 to $1; \
  \
 source(0).node.address(0).category=ipv4-addr; \
 source(0).node.address(0).address=$2; \
  \
 target(0).node.address(0).category = ipv4-addr; \
 target(0).node.address(0).address=$1; \
 last


